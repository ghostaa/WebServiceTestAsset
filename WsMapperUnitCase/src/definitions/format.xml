	</fmtDef>

	<!-- Context to Character -->
	<fmtDef id="fmt_Context2Character">
		<wsMapperConverter isPrimitive="false" javaClass="java.lang.Character">
			<mapper from="characterField" to="*" byReference="false"
				append="false" />
		</wsMapperConverter>
	</fmtDef>

	<!-- Context to JavaBean -->
	<fmtDef id="fmt_Context2JavaBean">
		<wsMapperConverter dataName="Ctxt_Context2JavaBean"
			isPrimitive="false" javaClass="com.ibm.btt.base.ws.format.test.bean.JavaBean">
			<mapper from="byteField" to="primitiveByte" byReference="false"
				append="ignore" />
			<mapper from="charField" to="primitiveChar" byReference="true"
				append="ignore" />
			<mapper from="shortField" to="primitiveShort" byReference="false"
				append="ignore" />
			<mapper from="intField" to="primitiveInt" byReference="false"
				append="ignore" />
			<mapper from="longField" to="primitiveLong" byReference="false"
				append="ignore" />
			<mapper from="floatField" to="primitiveFloat" byReference="false"
				append="ignore" />
			<mapper from="doubleField" to="primitiveDouble" byReference="false"
				append="ignore" />

			<!--  mapper from="IColl_Context2JavaBean_ByteArray.*" to="primitiveByteArray.*"
				byReference="false" append="ignore" /-->
			<mapper from="IColl_Context2JavaBean_CharArray.*" to="primitiveCharArray.*"
				byReference="true" append="ignore" />
			<mapper from="IColl_Context2JavaBean_ShortArray.*" to="primitiveShortArray.*"
				byReference="false" append="ignore" />
			<mapper from="IColl_Context2JavaBean_IntArray.*" to="primitiveIntArray.*"
				byReference="false" append="ignore" />
			<mapper from="IColl_Context2JavaBean_LongArray.*" to="primitiveLongArray.*"
				byReference="false" append="ignore" />
			<mapper from="IColl_Context2JavaBean_FloatArray.*" to="primitiveFloatArray.*"
				byReference="false" append="ignore" />
			<mapper from="IColl_Context2JavaBean_DoubleArray.*" to="primitiveDoubleArray.*"
				byReference="false" append="ignore" />

			<mapper from="strObjectField" to="stringObject" byReference="false"
				append="ignore" />

			<mapper from="IColl_Context2JavaBean_StringArray.*" to="stringObjectArray.*"
				byReference="false" append="ignore" />

			<mapper from="IColl_Context2JavaBean_ArrayList.*" to="stringObjectList.*"
				byReference="false" append="ignore" />

			<mapper
				from="KColl_Context2JavaBean_SubJavaBean.strObjectFieldInSubJavaBean"
				to="subJavaBean.someThing" byReference="false" append="ignore" />
			<mapper from="KColl_Context2JavaBean_SubJavaBean.intFieldInSubJavaBean"
				to="subJavaBean.intValue" byReference="false" append="ignore" />

			<mapper
				from="IColl_Context2JavaBean_SubJavaBeanArray.*.strObjectFieldInSubJavaBean"
				to="subJavaBeanArray.*.someThing" byReference="false" append="ignore" />
			<mapper
				from="IColl_Context2JavaBean_SubJavaBeanArray.*.intFieldInSubJavaBean"
				to="subJavaBeanArray.*.intValue" byReference="false" append="ignore" />

			<mapper
				from="IColl_Context2JavaBean_SubJavaBeanList.*.strObjectFieldInSubJavaBean"
				to="subJavaBeanList.*.someThing" byRefernce="false" append="ignore" />
			<mapper
				from="IColl_Context2JavaBean_SubJavaBeanList.*.intFieldInSubJavaBean"
				to="subJavaBeanList.*.intValue" byRefernce="false" append="ignore" />

		</wsMapperConverter>
	</fmtDef>


	<!--
		*************For new UT cases(Object to Context)****************
	-->

	<!-- java.util.List to Context -->
	<fmtDef id="fmt_List2Context">
		<wsMapperConverter isPrimitive="false"
			javaClass="java.util.List[java.lang.String]">
			<mapper from="array.*" to="IColl_List2Context.*" byReference="true"
				append="false" />
		</wsMapperConverter>
	</fmtDef>


	<!-- String[] to Context -->
	<fmtDef id="fmt_StringArray2Context">
		<wsMapperConverter isPrimitive="false" javaClass="java.lang.String[]">
			<mapper from="array.*" to="IColl_StringArray2Context.*"
				byReference="false" append="false" />
		</wsMapperConverter>
	</fmtDef>

	<!-- Bean[] to Context -->
	<fmtDef id="fmt_BeanArray2Context">
		<wsMapperConverter isPrimitive="false"
			javaClass="com.ibm.base.ws.format.bean.Bean[]">
			<mapper from="array.*.name" to="IColl_BeanArray2Context.*.strObjectFieldInBean"
				byReference="false" append="false" />
			<mapper from="array.*.age" to="IColl_BeanArray2Context.*.intFieldInBean"
				byReference="false" append="flase" />
		</wsMapperConverter>
	</fmtDef>

	<!-- byte[] to Context -->
	<fmtDef id="fmt_ByteArray2Context">
		<wsMapperConverter isPrimitive="false" javaClass="byte[]">
			<mapper from="array.*" to="IColl_ByteArray2Context.*"
				byReference="false" append="false" />
		</wsMapperConverter>
	</fmtDef>

