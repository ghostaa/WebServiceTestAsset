	<!-- Context to String -->
	<fmtDef id="fmt_Context2String">
		<wsMapperConverter isPrimitive="false" javaClass="java.lang.String">
			<mapper from="strField" to="*" byReference="false" append="false" />
		</wsMapperConverter>
	</fmtDef>

	<!-- Context to Boolean -->
	<fmtDef id="fmt_Context2Boolean">
