<?xml version="1.0" encoding="UTF-8"?>
<format.xml>
	
	<!-- *************For new UT cases(Context to Object)**************** -->

	<!-- Context to java.util.List -->
	<fmtDef id="fmt_Context2List">
		<wsMapperConverter dataName="KColl_Context2List"
			isPrimitive="false" javaClass="java.util.List[java.lang.String]">
			<mapper from="IColl_Context2List.*" to="array.*" byReference="false"
				append="false" />
		</wsMapperConverter>
	</fmtDef>

	<!-- Context to String[] -->
	<fmtDef id="fmt_Context2StringArray">
