	</fmtDef>

	<!-- String to Context -->
	<fmtDef id="fmt_String2Context">
		<wsMapperConverter isPrimitive="false" javaClass="java.lang.String">
			<mapper from="=*" to="stringObject" byReference="false"
				append="false" />
		</wsMapperConverter>
	</fmtDef>
	
	<fmtDef id="fmt_JavaBean2Context">
		<wsMapperConverter isPrimitive="false" javaClass="com.ibm.btt.base.ws.format.test.bean.JavaBean">
			<mapper from="primitiveByte" to="byteField" byReference="false" append="false" />
			<mapper from="primitiveChar" to="charField" byReference="false" append="false" />
			<mapper from="primitiveShort" to="shortField" byReference="false" append="false" />
			<mapper from="primitiveInt" to="intField" byReference="false" append="false" />
			<mapper from="primitiveLong" to="longField" byReference="false" append="false" />
			<mapper from="primitiveFloat" to="floatField" byReference="false" append="false" />
			<mapper from="primitiveDouble" to="doubleField" byReference="false" append="false" />
			
			<!-- mapper from="primitiveByteArray.*" to="IColl_JavaBean2Context_ByteArray.*" 
				byReference="false" append="false"/-->
			<mapper from="primitiveCharArray.*" to="IColl_JavaBean2Context_CharArray.*" 
				byReference="false" append="false"/>
			<mapper from="primitiveShortArray.*" to="IColl_JavaBean2Context_ShortArray.*" 
				byReference="false" append="false"/>
			<mapper from="primitiveIntArray.*" to="IColl_JavaBean2Context_IntArray.*" 
				byReference="false" append="false"/>
			<mapper from="primitiveLongArray.*" to="IColl_JavaBean2Context_LongArray.*" 
				byReference="false" append="false"/>
			<mapper from="primitiveFloatArray.*" to="IColl_JavaBean2Context_FloatArray.*" 
				byReference="false" append="false"/>
			<mapper from="primitiveDoubleArray.*" to="IColl_JavaBean2Context_DoubleArray.*" 
				byReference="false" append="false"/>
				
			<mapper from="stringObject" to="strObjectField" byReference="false" append="false" />
			
			<mapper from="stringObjectArray.*" to="IColl_JavaBean2Context_StringArray.*" 
				byReference="false" append="false"/>
				
			<mapper from="stringObjectList.*" to="IColl_JavaBean2Context_ArrayList.*" 
				byReference="false" append="false"/>
			
			<mapper from="subJavaBean.someThing" to="KColl_JavaBean2Context_SubJavaBean.strObjectFieldInSubJavaBean" byRefernce="false" append="false" />
			<mapper from="subJavaBean.intValue" to="KColl_JavaBean2Context_SubJavaBean.intFieldInSubJavaBean" byRefernce="false" append="false" />
			
			<mapper from="subJavaBeanArray.*.someThing" to="IColl_JavaBean2Context_SubJavaBeanArray.*.strObjectFieldInSubJavaBean" byReference="false" append="flase"/>
			<mapper from="subJavaBeanArray.*.intValue" to="IColl_JavaBean2Context_SubJavaBeanArray.*.intFieldInSubJavaBean" byReference="false" append="ignore"/>
		
			<mapper from="subJavaBeanList.*.someThing" to="IColl_JavaBean2Context_SubJavaBeanList.*.strObjectFieldInSubJavaBean" byReference="false" append="false"/>
			<mapper from="subJavaBeanList.*.intValue" to="IColl_JavaBean2Context_SubJavaBeanList.*.intFieldInSubJavaBean" byReference="false" append="ignore"/>
		
		</wsMapperConverter>
	</fmtDef>
</format.xml>
